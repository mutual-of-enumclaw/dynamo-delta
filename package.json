    "license": "MIT License",
    "main": "dist/src/index.js",
    "scripts": {
        "prebuild": "npm run lt",
        "build": "npm run package",
        "test": "npx jest",
        "test:coverage": "npm run test -- --coverage",
        "lint": "npx tslint -c tslint.json -p tsconfig.json",
        "lint:fix": "npx tslint -c tslint.json -p tsconfig.json --fix",
        "lt": "npm run lint && npm run test:coverage",
        "package": "npx tsc -d"
    },
    "dependencies": {
        "aws-sdk": "^2.444.0"
    },
    "devDependencies": {
        "@types/node": "^10.14.6",
        "@types/jest": "^24.0.12",
        "jest": "23.6.0",
        "jest-junit": "^6.2.1",
        "ts-jest": "^23.10.5",
        "ts-loader": "^5.4.4",
        "tslint": "^5.16.0",
        "typescript": "^3.4.5"
    },
    "files": [
        "/dist",
        "README.md"
    ]
}
